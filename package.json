{
  "name": "cardano-serialization-lib",
  "version": "1.0.0",
  "main": "browser.mjs",
  "types": "browser.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "node": {
        "types": "./node.d.ts",
        "require": "./node.cjs",
        "default": "./node.cjs"
      },
      "default": {
        "types": "./browser.d.ts",
        "import": "./browser.mjs",
        "require": "./node.cjs",
        "default": "./browser.mjs"
      }
    }
  },
  "dependencies": {
    "@emurgo/cardano-serialization-lib-browser": "^12.0.1",
    "@emurgo/cardano-serialization-lib-nodejs": "^12.0.1"
  }
}